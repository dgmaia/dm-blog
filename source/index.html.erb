---
pageable: true
per_page: 10
---

<% page_articles.each_with_index do |article, i| %>
  <article>
    <header>
      <h2><%= link_to article.title, article %></h2>
      <p class="date">
        <%= article.date.strftime('%B %e %Y') %>
      </p>
    </header>
    <section class="body">
      <%= article.summary %>
    </section>
  </article>
<% end %>
